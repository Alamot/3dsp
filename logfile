perl -pi -e 's#^CONF_FILE\s+=.*#CONF_FILE = "/etc/wifi-radar/wifi-radar.conf"#' wifi-radar.tmp
test -e /sbin/wpa_supplicant || perl -pi -e 's#^WPA_SUPPLICANT_COMMAND\s+=.*#WPA_SUPPLICANT_COMMAND = "/usr/sbin/wpa_supplicant"#' wifi-radar.tmp
mv wifi-radar.tmp wifi-radar.localized
cp wifi-radar.desktop wifi-desktop.tmp
test ! -e /usr/bin/su-to-root || test ! -e /usr/bin/gksudo || perl -pi -e 's|^Exec=.*|Exec=su-to-root -X -c /usr/sbin/3dsp-wifi-radar|' wifi-desktop.tmp
test -e /usr/bin/su-to-root || test -e /usr/bin/gksudo || perl -pi -e 's|^Exec=.*|Exec=3dsp-wifi-radar-pam|' wifi-desktop.tmp
ln -sf /usr/bin/consolehelper /usr/sbin/3dsp-wifi-radar-pam
echo USER=root > wifi-radar-pam.tmp
echo PROGRAM=/usr/sbin/3dsp-wifi-radar >> wifi-radar-pam.tmp
echo SESSION=true >> wifi-radar-pam.tmp
echo "#%PAM-1.0" > wifi-radar-pam.tp2
echo "auth       sufficient   pam_rootok.so" >> wifi-radar-pam.tp2
echo "auth       sufficient   pam_timestamp.so" >> wifi-radar-pam.tp2
echo "#auth       required     pam_stack.so service=system-auth" >> wifi-radar-pam.tp2
echo "auth       required     pam_console.so" >> wifi-radar-pam.tp2
echo "session    required     pam_permit.so" >> wifi-radar-pam.tp2
echo "session    optional     pam_xauth.so" >> wifi-radar-pam.tp2
echo "session    optional     pam_timestamp.so" >> wifi-radar-pam.tp2
echo "account    required     pam_permit.so" >> wifi-radar-pam.tp2
test -d /etc/wifi-radar || install -d -m 755 /etc/wifi-radar
test -e /etc/wifi-radar/wifi-radar.conf || install -m 644 wifi-radar.conf	/etc/wifi-radar
test -e /etc/wifi-radar/Makefile || install -m 644 Makefile	/etc/wifi-radar
test -d /usr/sbin || install -d -m 755 /usr/sbin
install -m 755 wifi-radar.localized	/usr/sbin/3dsp-wifi-radar
rm wifi-radar.localized
test -d /usr/share/doc/3dsp-wifi-radar || install -d -m 755 /usr/share/doc/3dsp-wifi-radar
install -m 644 doc/3dsp-wifi-radar.html 		/usr/share/doc/3dsp-wifi-radar
test -d /usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files || install -d -m 755 /usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image001.jpg 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image002.gif 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image003.gif 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image004.jpg 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image005.jpg 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image006.jpg 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image007.jpg 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
install -m 644 doc/3dsp-wifi-radar.files/image008.jpg 		/usr/share/doc/3dsp-wifi-radar/3dsp-wifi-radar.files
test -d /etc/init.d || install -d -m 755 /etc/init.d
install -m 755 init_script 		/etc/init.d/wifi-radar
test -d /usr/share/pixmaps || install -d -m 755 /usr/share/pixmaps
install -m 644 pixmaps/wifi-radar.svg	/usr/share/pixmaps
install -m 644 pixmaps/wifi-radar.png	/usr/share/pixmaps
install -m 644 pixmaps/wifi_radar_32x32.png	/usr/share/pixmaps
install -m 644 pixmaps/connected1.png	/usr/share/pixmaps
install -m 644 pixmaps/connected2.png	/usr/share/pixmaps
install -m 644 pixmaps/connected3.png	/usr/share/pixmaps
install -m 644 pixmaps/connected4.png	/usr/share/pixmaps
test -d /usr/share/applications || install -d -m 755 /usr/share/applications
install -m 644 wifi-desktop.tmp		/usr/share/applications/wifi-radar.desktop
rm -f wifi-desktop.tmp
test -e /sbin/wpa_supplicant || test -d /etc/pam.d || install -d -m 755 /etc/pam.d
test -e /sbin/wpa_supplicant || install -m 644 wifi-radar-pam.tp2 /etc/pam.d/3dsp-wifi-radar-pam
test ! -e wifi-radar-pam.tp2 || rm wifi-radar-pam.tp2
test -e /sbin/wpa_supplicant || test -d /etc/security/console.apps || install -d -m 755 /etc/security/console.apps
test -e /sbin/wpa_supplicant || install -m 644 wifi-radar-pam.tmp /etc/security/console.apps/3dsp-wifi-radar-pam
test ! -e wifi-radar-pam.tmp || rm wifi-radar-pam.tmp
test -e /sbin/iwconfig || test ! -d packages || test ! -e packages/libiw28_28-1+etchnhalf.1_i386.deb || dpkg -i -GE packages/libiw28_28-1+etchnhalf.1_i386.deb
test -e /sbin/iwconfig || test ! -d packages || test ! -e packages/wireless-tools_28-1+etchnhalf.1_i386.deb || dpkg -i -GE packages/wireless-tools_28-1+etchnhalf.1_i386.deb
test -e /usr/bin/python || test ! -d packages || test ! -e packages/python-notify_0.1.1-2+b1_i386.deb || dpkg -i -GE packages/python-notify_0.1.1-2+b1_i386.deb
